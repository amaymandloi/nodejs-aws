{"ast":null,"code":"import Vue from \"vue\";\nimport cors from \"cors\";\nVue.use(cors);\nexport default {\n  name: \"EditItem\",\n  props: [\"editedIndex\", \"defaultItem\", \"item\", \"dialog\", \"editedItem\", \"farms\"],\n  data() {\n    return {\n      proxyEditedItem: {\n        farm_id: \"\",\n        farm_name: \"\",\n        cit: \"\",\n        lat: \"\",\n        long: \"\",\n        country: \"\"\n      },\n      proxyEditedIndex: -1\n    };\n  },\n  created() {\n    this.proxyEditedItem = this.editedItem;\n    this.proxyEditedIndex = this.editedIndex;\n    this.proxyDialog = this.dialog;\n  },\n  methods: {\n    editTableItem(proxyEditedItem, proxyEditedIndex, proxyDialog) {\n      this.$emit(\"sendData\", proxyEditedItem, proxyEditedIndex, proxyDialog);\n    },\n    editItem(item) {\n      this.proxyEditedIndex = this.farms.indexOf(item);\n      this.proxyEditedItem = Object.assign({}, item);\n      this.proxyDialog = true;\n    },\n    async save() {\n      const editItemData = this.proxyEditedItem;\n      console.log(editItemData);\n      const res = await fetch(\"https://lugg9ewkq6.execute-api.us-east-1.amazonaws.com/Test/farm\", {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify(editItemData)\n      });\n      const data = await res.json();\n      this.close();\n    },\n    close() {\n      this.proxyDialog = false;\n      this.$nextTick(() => {\n        this.proxyEditedItem = Object.assign({}, this.defaultItem);\n        this.proxyEditedIndex = -1;\n      });\n    }\n  },\n  watch: {\n    dialog(val) {\n      val || this.close();\n    }\n  },\n  computed: {\n    formTitle() {\n      return this.proxyEditedIndex === -1 ? \"New Item\" : \"Edit Item\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAyDA;AACA;AACAA;AAEA;EACAC;EACAC,QACA,eACA,eACA,QACA,UACA,cACA,QACA;EACAC;IACA;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACAC;IACA;EACA;EACAC;IACA;IACA;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACA;MACA;MACAC;MACA,wBACA,oEACA;QACAC;QACAC;UACA;QACA;QACAC;MACA,EACA;MACA;MACA;IACA;IACAC;MACA;MACA;QACA;QACA;MACA;IACA;EACA;EACAC;IACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;AACA","names":["Vue","name","props","data","proxyEditedItem","farm_id","farm_name","cit","lat","long","country","proxyEditedIndex","created","methods","editTableItem","editItem","console","method","headers","body","close","watch","dialog","val","computed","formTitle"],"sourceRoot":"src/components","sources":["EditItem.vue"],"sourcesContent":["<template>\n  <v-dialog v-model=\"proxyDialog\" max-width=\"500px\">\n    <v-card>\n      <v-card-title>\n        <span class=\"text-h5\">{{ formTitle }}</span>\n      </v-card-title>\n      <v-card-text>\n        <v-container>\n          <v-row>\n            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n              <v-text-field\n                v-model=\"proxyEditedItem.farm_id\"\n                label=\"Farm ID\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n              <v-text-field\n                v-model=\"proxyEditedItem.farm_name\"\n                label=\"Farm Name\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n              <v-text-field\n                v-model=\"proxyEditedItem.city\"\n                label=\"City\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n              <v-text-field\n                v-model=\"proxyEditedItem.lat\"\n                label=\"lat\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n              <v-text-field\n                v-model=\"proxyEditedItem.long\"\n                label=\"long\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n              <v-text-field\n                v-model=\"proxyEditedItem.county\"\n                label=\"country\"\n              ></v-text-field>\n            </v-col>\n          </v-row>\n        </v-container>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"blue darken-1\" text @click=\"close\"> Cancel </v-btn>\n        <v-btn color=\"blue darken-1\" text @click=\"save\"> Save </v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n<script>\nimport Vue from \"vue\";\nimport cors from \"cors\";\nVue.use(cors);\n\nexport default {\n  name: \"EditItem\",\n  props: [\n    \"editedIndex\",\n    \"defaultItem\",\n    \"item\",\n    \"dialog\",\n    \"editedItem\",\n    \"farms\",\n  ],\n  data() {\n    return {\n      proxyEditedItem: {\n        farm_id: \"\",\n        farm_name: \"\",\n        cit: \"\",\n        lat: \"\",\n        long: \"\",\n        country: \"\",\n      },\n      proxyEditedIndex: -1,\n    };\n  },\n  created() {\n    this.proxyEditedItem = this.editedItem;\n    this.proxyEditedIndex = this.editedIndex;\n    this.proxyDialog = this.dialog;\n  },\n  methods: {\n    editTableItem(proxyEditedItem, proxyEditedIndex, proxyDialog) {\n      this.$emit(\"sendData\", proxyEditedItem, proxyEditedIndex, proxyDialog);\n    },\n    editItem(item) {\n      this.proxyEditedIndex = this.farms.indexOf(item);\n      this.proxyEditedItem = Object.assign({}, item);\n      this.proxyDialog = true;\n    },\n    async save() {\n      const editItemData = this.proxyEditedItem;\n      console.log(editItemData)\n      const res = await fetch(\n        \"https://lugg9ewkq6.execute-api.us-east-1.amazonaws.com/Test/farm\",\n        {\n          method: \"POST\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n          },\n          body: JSON.stringify(editItemData),\n        }\n      );\n      const data = await res.json();\n      this.close();\n    },\n    close() {\n      this.proxyDialog = false;\n      this.$nextTick(() => {\n        this.proxyEditedItem = Object.assign({}, this.defaultItem);\n        this.proxyEditedIndex = -1;\n      });\n    },\n  },\n  watch: {\n    dialog(val) {\n      val || this.close();\n    },\n  },\n  computed: {\n    formTitle() {\n      return this.proxyEditedIndex === -1 ? \"New Item\" : \"Edit Item\";\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}